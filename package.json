{
  "name": "synapse",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "db": "turbo db:push db:generate --no-daemon",
    "build": "turbo run build --no-daemon",
    "dev": "turbo run dev --no-daemon",
    "lint": "turbo run lint --no-daemon",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-synapse": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.27.0"
}
