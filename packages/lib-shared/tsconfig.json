{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "CommonJS",
    "types": ["@types/node"],
    "lib": ["ES2020"],
    "strictPropertyInitialization": false
  },
  "exclude": ["node_modules"]
}
